<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <title>Notícias</title>
</head>
<body>
    <h1>Notícias</h1>
    <table border="1px">
        <tr>
            <th>Id</th>
            <th>Título</th>
            <th>Acao</th>

        </tr>
       <% for(let i = 0; i < noticias.length; i++) { %>
          <tr>
              <td><%= noticias[i].id_noticia %></td>
              <td><%= noticias[i].titulo %></td>
              <td><a href="#">Visualizar</a></td>
          </tr>
          <% } %>
        </table>
</body>
</html>